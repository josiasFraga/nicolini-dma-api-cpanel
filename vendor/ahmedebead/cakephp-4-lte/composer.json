{
    "name": "ahmedebead/cakephp-4-lte",
    "description": "CakePHP 4.x AdminLTE 3.1 Theme.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "templates",
        "plugin"
    ],
    "homepage": "https://github.com/ahmed3bead/cakephp-4-lte",
    "license": "MIT",
    "authors": [{
        "name": "Ahmed Ebead",
        "email": "ahmedm3bead@gmail.com",
        "homepage": "https://github.com/ahmed3bead",
        "role": "Author"
    }],
    "require": {
        "php": ">=7.2.0",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakephpLte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpLte\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "support": {
        "issues": "https://github.com/ahmed3bead/cakephp-4-lte/issues",
        "source": "https://github.com/ahmed3bead/cakephp-4-lte"
    }
}
